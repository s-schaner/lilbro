{
  "id": "screensnap-insights",
  "name": "Screenshot Insights",
  "version": "0.2.0",
  "frontend": {
    "mount": "right/image-analysis",
    "routes": ["/screensnap"]
  },
  "backend": {
    "router": "app/modules/screensnap/router.py",
    "env": ["LLM_BASE_URL", "LLM_MODEL", "LLM_API_KEY"]
  },
  "permissions": ["read:video_frame", "write:temp", "net:http"],
  "health": {"endpoint": "/screensnap/health", "interval_sec": 20},
  "optional": true,
  "enabled_by_default": true
}
